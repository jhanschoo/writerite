{
	"name": "@writerite/backend-apollo",
	"version": "0.1.0",
	"description": "Apollo GraphQL implementation of WriteRite backend",
	"main": "dist/index.js",
	"repository": "https://gitlab.com/writerite/writerite",
	"author": "Johannes Choo",
	"license": "AGPL-3.0-or-later",
	"private": false,
	"dependencies": {
		"@prisma/client": "^2.29.1",
		"@types/bcrypt": "^5.0.0",
		"@types/ioredis": "^4.26.4",
		"@types/koa-helmet": "^6.0.2",
		"@types/node": "^14.17.9",
		"@types/node-fetch": "^2.5.10",
		"@types/promise-dag": "^1.0.1",
		"@types/ws": "^7.2.6",
		"apollo": "^2.33.3",
		"apollo-server-core": "^3.1.2",
		"apollo-server-koa": "^3.1.2",
		"bcrypt": "^5.0.0",
		"csv": "^5.5.0",
		"dotenv": "^10.0.0",
		"form-data": "^4.0.0",
		"google-auth-library": "^7.1.0",
		"graphql": "^15.3.0",
		"graphql-redis-subscriptions": "^2.4.0",
		"graphql-scalars": "^1.9.3",
		"ioredis": "^4.27.3",
		"jsrsasign": "^10.2.0",
		"koa": "^2.13.1",
		"koa-helmet": "^6.1.0",
		"moment": "^2.29.1",
		"nanoid": "^3.1.23",
		"nexus": "^1.0.0",
		"node-fetch": "^2.6.1",
		"promise-dag": "^1.0.0",
		"subscriptions-transport-ws": "^0.9.18",
		"typescript": "^4.2.4"
	},
	"devDependencies": {
		"@types/jest": "^27.0.0",
		"@types/random-words": "^1.1.1",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"draft-js": "^0.11.7",
		"eslint": "^7.27.0",
		"eslint-plugin-jest": "^24.4.0",
		"eslint-plugin-jsdoc": "^36.0.7",
		"graphql-subscriptions": "^1.1.0",
		"jest": "^27.0.1",
		"prisma": "^2.29.1",
		"ts-jest": "^27.0.0",
		"ts-node": "^10.0.0",
		"ts-node-dev": "^1.1.6"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/index.js",
		"dev": "export $(xargs -a .env -d '\\n') && ts-node-dev --transpile-only --no-notify src/index.ts",
		"generate": "export $(xargs -a .env -d '\\n') && prisma generate && ts-node --transpile-only src/schema",
		"test:integration": "export $(xargs -a .env -d '\\n') && npm run generate && jest --runInBand test/integration",
		"push-schema:ci": "apollo service:push --graph=writerite-graph -v \"$CI_COMMIT_REF_SLUG\" --localSchemaFile=generated/schema.graphql",
		"push-schema:dev": "export $(xargs -a .env -d '\\n') && apollo service:push --graph=writerite-graph -v \"$APOLLO_GRAPH_VARIANT\" --localSchemaFile=generated/schema.graphql"
	}
}
