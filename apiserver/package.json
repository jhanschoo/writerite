{
  "name": "@writerite/apiserver",
  "version": "0.1.0",
  "description": "GraphQL Yoga implementation of WriteRite backend",
  "main": "dist/index.js",
  "repository": "https://gitlab.com/writerite/writerite",
  "author": "Johannes Choo",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "dependencies": {
    "@graphql-codegen/add": "^4.0.1",
    "@graphql-codegen/cli": "^3.2.2",
    "@graphql-codegen/typescript": "^3.0.2",
    "@graphql-codegen/typescript-operations": "^3.0.2",
    "@graphql-yoga/redis-event-target": "^1.0.0",
    "@prisma/client": "^4.7.1",
    "@tsconfig/node18": "^1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^18.11.4",
    "@types/promise-dag": "^1.0.1",
    "bcrypt": "^5.0.0",
    "form-data": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.9.3",
    "graphql-ws": "^5.12.0",
    "graphql-yoga": "^3.7.3",
    "ioredis": "^5.2.3",
    "jose": "^4.10.0",
    "nanoid": "^3.3.4",
    "nexus": "^1.3.0",
    "promise-dag": "^1.0.0",
    "typescript": "^4.9.5",
    "ws": "^8.7.0",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@graphql-tools/executor-http": "^0.1.9",
    "@types/jest": "^29.5.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "dotenv": "^16.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-jsdoc": "^40.0.2",
    "jest": "^29.1.2",
    "jest-mock-extended": "^3.0.1",
    "prettier": "^2.7.1",
    "prisma": "^4.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "build": "tsc",
    "init:run": "prisma db push --accept-data-loss",
    "app:start": "node dist/src/index.js",
    "dev": "export $(xargs -a .env -d '\\n') && ts-node-dev --exit-child --transpile-only --respawn src/index.ts",
    "dev:init": "export $(xargs -a .env -d '\\n') && prisma db push && prisma generate && graphql-codegen && ts-node --transpile-only src/schema",
    "test:integration": "export $(xargs -a .env -d '\\n') && graphql-codegen && jest --runInBand --config=\"jest.config.integration.ts\" test/integration",
    "test:unit": "graphql-codegen && jest"
  }
}
