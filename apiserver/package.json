{
  "name": "@writerite/apiserver",
  "version": "0.1.0",
  "description": "GraphQL Yoga implementation of WriteRite backend",
  "main": "dist/index.js",
  "repository": "https://gitlab.com/writerite/writerite",
  "author": "Johannes Choo",
  "license": "AGPL-3.0-or-later",
  "private": false,
  "dependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.11",
    "@graphql-codegen/typescript-operations": "^2.4.0",
    "@graphql-yoga/node": "^2.9.0",
    "@prisma/client": "^3.15.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/ioredis": "^4.26.4",
    "@types/node": "^16.11.38",
    "@types/promise-dag": "^1.0.1",
    "bcrypt": "^5.0.0",
    "cross-fetch": "^3.1.5",
    "csv": "^6.1.0",
    "cuid": "^2.1.8",
    "dotenv": "^16.0.1",
    "form-data": "^4.0.0",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.9.3",
    "graphql-ws": "^5.8.2",
    "ioredis": "^4.27.3",
    "jsrsasign": "^10.2.0",
    "nanoid": "^3.1.23",
    "nexus": "^1.3.0",
    "promise-dag": "^1.0.0",
    "typescript": "^4.7.3",
    "ws": "^8.7.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "draft-js": "^0.11.7",
    "eslint": "^8.17.0",
    "eslint-plugin-jest": "^25.7.0",
    "eslint-plugin-jsdoc": "^37.9.7",
    "jest": "^28.1.0",
    "jest-mock-extended": "^2.0.4",
    "prisma": "^3.15.2",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "build": "tsc",
    "init:run": "prisma db push --accept-data-loss",
    "app:start": "node dist/src/index.js",
    "dev": "export $(xargs -a .env -d '\\n') && ts-node-dev --exit-child --transpile-only --respawn src/index.ts",
    "dev:init": "export $(xargs -a .env -d '\\n') && prisma db push && prisma generate && graphql-codegen && ts-node --transpile-only src/schema",
    "test:integration": "export $(xargs -a .env -d '\\n') && graphql-codegen && jest --runInBand test/integration",
    "test:unit": "export $(xargs -a .env.unit-test -d '\\n') && graphql-codegen && jest --runInBand test/unit"
  }
}
