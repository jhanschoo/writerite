	"scripts": {
		"build": "tsc",
		"init:run": "prisma db push",
		"app:start": "node dist/src/index.js",
		"dev": "export $(xargs -a .env -d '\\n') && ts-node-dev --transpile-only --no-notify src/index.ts",
		"dev:init": "export $(xargs -a .env -d '\\n') && prisma db push && prisma generate && ts-node --transpile-only src/schema",
		"test:integration": "export $(xargs -a .env -d '\\n') && jest --runInBand test/integration",
		"test:unit": "export $(xargs -a .env.unit-test -d '\\n') && jest --runInBand test/unit"
	}
