{
  "extends": "@tsconfig/node18/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "outDir": "./dist/",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types", "./typings"],
    "noErrorTruncation": true,
    "skipLibCheck": true /* Skip type checking of declaration files. */
  },
  "include": ["generated/**/*", "src/**/*"]
}
