  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "name": "Launch Program",
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
