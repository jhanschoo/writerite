  "scripts": {
    "start": "npm run codegen && npm run download-schema && rescripts start",
    "build": "npm run codegen && npm run download-schema && rescripts build",
    "test": "rescripts test",
    "test:debug": "rescripts --inspect-brk test --runInBand",
    "eject": "rescripts eject",
    "copymodels": "cp -R ../client-models/ src/",
    "codegen": "npm run copymodels && apollo client:codegen --useReadOnlyTypes --globalTypesFile=src/gqlGlobalTypes.ts --target=typescript gqlTypes",
    "download-schema": "apollo client:download-schema src/schema.json"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
