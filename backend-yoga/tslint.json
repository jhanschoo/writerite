{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "linterOptions": {
        "exclude": [
            "generated/**/*.ts"
        ]
    },
    "rules": {
      "quotemark": [true, "single"],
      "indent": [true, "spaces", 2],
      "ordered-imports": false,
      "object-literal-sort-keys": false,
      "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
    },
    "rulesDirectory": []
}
